{
  "expo": {
    "name": "frontend",
    "slug": "frontend",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/adaptive-icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./app/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.anonymous.frontend"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "./plugins/withReactNativeHce",
        {
          "appIds": [
            "D2760000850101",
            "F0010203040506",
            "F0394148148100"
          ]
        }
      ],
      [
        "./plugins/withAndroidColorEdgeEffect",
        {
          "color": "#123456"
        }
      ],
      [
        "./plugins/withAndroidApplicationAttributes",
        {
          "android:usesCleartextTraffic": false
        }
      ],
      [
        "react-native-nfc-manager",
        {
          "nfcPermission": "Custom permission message",
          "selectIdentifiers": [
            "A0000002471001"
          ],
          "systemCodes": [
            "8008"
          ]
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "1e2c4944-c8fe-487f-89d2-24deb09a6793"
      }
    }
  }
}